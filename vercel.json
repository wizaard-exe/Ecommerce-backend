{
  "version": 2,
  "name": "backendDeployment",
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/(.*)", 
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*" 
        }
      ]
    }
  ]
}
